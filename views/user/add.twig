{% extends "layout.twig" %}

{% block body %} 
<div class="container">
    <div class="row">
        <div class="col-md-12">
            {{ form_start(form) }}
            
                {{ form_row(form.first_name) }}
                <br>
                
                {{ form_row(form.last_name) }}
                <br>
                
                {{ form_row(form.email) }}
                <br>
                
                <div class="well well-sm form-actions">
                {{ form_row(form.save) }}
                </div>
            
            {{ form_end(form) }}
        </div>
    </div>
</div>
{%endblock %}